#!/bin/bash
 declare BRAINLINTFILES=$(cat /home/noah/Developer/visual_computing_openwalnut/build/brainlint/brainlintlist_transferFunction2D); for B_FILE in $BRAINLINTFILES; do B_FILE=$(echo $B_FILE | tr -d ''); /home/noah/Developer/visual_computing_openwalnut/src/../tools/style/brainlint/brainlint.py --color $B_FILE;done 2>&1| grep -iv 'Total errors found: 0$'|grep -qi 'Total';! $(exit $?)