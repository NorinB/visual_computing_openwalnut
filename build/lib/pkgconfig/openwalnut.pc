prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: openwalnut
Description: The OpenWalnut core library -- OpenWalnut is a tool for multi-modal medical and brain data visualization.
Version: 1.2.5
Cflags: -I${includedir}/openwalnut -DEIGEN_DONT_VECTORIZE;-DEIGEN_DONT_ALIGN;-DEIGEN_DISABLE_UNALIGNED_ARRAY_ASSERT
Libs: -L${libdir} -lopenwalnut
Requires: openscenegraph eigen3
